/ul>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport EditItem from \"./EditItem.vue\";\n\nexport default {\n  components: {\n    EditItem,\n  },\n  data() {\n    return {\n      items: [],\n      inEdit: 0,\n      newItemName: \"\",\n    };\n  },\n  mounted() {\n    axios.get(`api/items`).then(response => {\n      this.items = response.data;\n    });\n  },\n  methods: {\n    createItem() {\n      axios\n        .post(`api/items`, { name: this.newItemName })\n        .then(response => {\n          this.items.push(response.data);\n          this.newItemName = \"\";\n        });\n    },\n    deleteItem(itemId) {\n      axios.delete(`api/items/${itemId}`).then(() => {\n        this.items = this.items.filter(item => item.id !== itemId);\n      });\n    },\n    updateItem(item) {\n      axios\n        .put(`api/items/${this.id}`, { id: item.id, name: item.name });\n    }\n  }\n};\n</script>\n"],"mappings":";AAqBA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,QAAO,MAAO,gBAAgB;AAErC,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE,CAAC;MACTC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRP,KAAK,CAACQ,GAAG,CAAE,WAAU,CAAC,CAACC,IAAI,CAACC,QAAO,IAAK;MACtC,IAAI,CAACN,KAAI,GAAIM,QAAQ,CAACP,IAAI;IAC5B,CAAC,CAAC;EACJ,CAAC;EACDQ,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACXZ,KAAI,CACDa,IAAI,CAAE,WAAU,EAAE;QAAEC,IAAI,EAAE,IAAI,CAACR;MAAY,CAAC,EAC5CG,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACN,KAAK,CAACW,IAAI,CAACL,QAAQ,CAACP,IAAI,CAAC;QAC9B,IAAI,CAACG,WAAU,GAAI,EAAE;MACvB,CAAC,CAAC;IACN,CAAC;IACDU,UAAUA,CAACC,MAAM,EAAE;MACjBjB,KAAK,CAACkB,MAAM,CAAE,aAAYD,MAAO,EAAC,CAAC,CAACR,IAAI,CAAC,MAAM;QAC7C,IAAI,CAACL,KAAI,GAAI,IAAI,CAACA,KAAK,CAACe,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACC,EAAC,KAAMJ,MAAM,CAAC;MAC5D,CAAC,CAAC;IACJ,CAAC;IACDK,UAAUA,CAACF,IAAI,EAAE;MACfpB,KAAI,CACDuB,GAAG,CAAE,aAAY,IAAI,CAACF,EAAG,EAAC,EAAE;QAAEA,EAAE,EAAED,IAAI,CAACC,EAAE;QAAEP,IAAI,EAAEM,IAAI,CAACN;MAAK,CAAC,CAAC;IAClE;EACF;AACF,CAAC"},"metadata":{},"source