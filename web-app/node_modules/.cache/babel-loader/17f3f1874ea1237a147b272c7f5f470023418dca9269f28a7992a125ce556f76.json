{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      items: [],\n      newItemName: \"\"\n    };\n  },\n  mounted() {\n    axios.get(`api/items`).then(response => {\n      console.log(response.data);\n      this.items = response.data;\n    });\n  },\n  methods: {\n    updateItem(itemId, newItemName) {\n      const csrfToken = document.querySelector('meta[name=\"csrf-token\"]').getAttribute('content');\n      axios.interceptors.request.use(function (config) {\n        config.headers['X-CSRF-Token'] = csrfToken;\n        return config;\n      });\n      axios.put(`api/items`, {\n        id: itemId,\n        name: newItemName\n      });\n    },\n    createItem() {\n      axios.post(`api/items`, {\n        name: this.newItemName\n      }).then(response => {\n        this.items.push(response.data);\n        this.newItemName = \"\";\n      });\n    },\n    deleteItem(itemId) {\n      axios.delete(`api/items/${itemId}`).then(() => {\n        this.items = this.items.filter(item => item.id !== itemId);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","data","items","newItemName","mounted","get","then","response","console","log","methods","updateItem","itemId","csrfToken","document","querySelector","getAttribute","interceptors","request","use","config","headers","put","id","name","createItem","post","push","deleteItem","delete","filter","item"],"sources":["/Users/omid/Projects/dmg/web-app/src/components/ItemsList.vue"],"sourcesContent":["<template>\n  <div>\n    <h1>Items</h1>\n    <form @submit.prevent=\"createItem\">\n      <input type=\"text\" v-model=\"newItemName\" />\n      <button type=\"submit\">Create Item</button>\n    </form>\n    <ul>\n      <li v-for=\"item in items\" :key=\"item.id\">\n        <input type=\"text\" :value=item.name />\n        <button @click=\"updateItem(item.id, item.name)\">Update</button>\n        <button @click=\"deleteItem(item.id)\">Delete</button>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      items: [],\n      newItemName: \"\",\n    };\n  },\n  mounted() {\n    axios.get(`api/items`).then(response => {\n      console.log(response.data);\n      this.items = response.data;\n    });\n  },\n  methods: {\n    updateItem(itemId, newItemName) {\n      const csrfToken = document.querySelector('meta[name=\"csrf-token\"]').getAttribute('content');\n\n      axios.interceptors.request.use(function (config) {\n        config.headers['X-CSRF-Token'] = csrfToken;\n        return config;\n      });\n\n      axios.put(`api/items`, { id: itemId, name: newItemName })\n    },\n    createItem() {\n      axios\n        .post(`api/items`, { name: this.newItemName })\n        .then(response => {\n          this.items.push(response.data);\n          this.newItemName = \"\";\n        });\n    },\n    deleteItem(itemId) {\n      axios.delete(`api/items/${itemId}`).then(() => {\n        this.items = this.items.filter(item => item.id !== itemId);\n      });\n    }\n  }\n};\n</script>\n"],"mappings":";AAkBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRJ,KAAK,CAACK,GAAG,CAAE,WAAU,CAAC,CAACC,IAAI,CAACC,QAAO,IAAK;MACtCC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACN,IAAI,CAAC;MAC1B,IAAI,CAACC,KAAI,GAAIK,QAAQ,CAACN,IAAI;IAC5B,CAAC,CAAC;EACJ,CAAC;EACDS,OAAO,EAAE;IACPC,UAAUA,CAACC,MAAM,EAAET,WAAW,EAAE;MAC9B,MAAMU,SAAQ,GAAIC,QAAQ,CAACC,aAAa,CAAC,yBAAyB,CAAC,CAACC,YAAY,CAAC,SAAS,CAAC;MAE3FhB,KAAK,CAACiB,YAAY,CAACC,OAAO,CAACC,GAAG,CAAC,UAAUC,MAAM,EAAE;QAC/CA,MAAM,CAACC,OAAO,CAAC,cAAc,IAAIR,SAAS;QAC1C,OAAOO,MAAM;MACf,CAAC,CAAC;MAEFpB,KAAK,CAACsB,GAAG,CAAE,WAAU,EAAE;QAAEC,EAAE,EAAEX,MAAM;QAAEY,IAAI,EAAErB;MAAY,CAAC;IAC1D,CAAC;IACDsB,UAAUA,CAAA,EAAG;MACXzB,KAAI,CACD0B,IAAI,CAAE,WAAU,EAAE;QAAEF,IAAI,EAAE,IAAI,CAACrB;MAAY,CAAC,EAC5CG,IAAI,CAACC,QAAO,IAAK;Q