},\n  methods: {\n    updateItem(itemId, newItemName) {\n      axios.put(`api/items`, { id: itemId, name: newItemName })\n    },\n    createItem() {\n      axios\n        .post(`api/items`, { name: this.newItemName })\n        .then(response => {\n          this.items.push(response.data);\n          this.newItemName = \"\";\n        });\n    },\n    deleteItem(itemId) {\n      axios.delete(`api/items/${itemId}`).then(() => {\n        this.items = this.items.filter(item => item.id !== itemId);\n      });\n    }\n  }\n};\n</script>\n"],"mappings":";AAkBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRJ,KAAK,CAACK,GAAG,CAAE,WAAU,CAAC,CAACC,IAAI,CAACC,QAAO,IAAK;MACtCC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACN,IAAI,CAAC;MAC1B,IAAI,CAACC,KAAI,GAAIK,QAAQ,CAACN,IAAI;IAC5B,CAAC,CAAC;EACJ,CAAC;EACDS,OAAO,EAAE;IACPC,UAAUA,CAACC,MAAM,EAAET,WAAW,EAAE;MAC9BH,KAAK,CAACa,GAAG,CAAE,WAAU,EAAE;QAAEC,EAAE,EAAEF,MAAM;QAAEG,IAAI,EAAEZ;MAAY,CAAC;IAC1D,CAAC;IACDa,UAAUA,CAAA,EAAG;MACXhB,KAAI,CACDiB,IAAI,CAAE,WAAU,EAAE;QAAEF,IAAI,EAAE,IAAI,CAACZ;MAAY,CAAC,EAC5CG,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACL,KAAK,CAACgB,IAAI,CAACX,QAAQ,CAACN,IAAI,CAAC;QAC9B,IAAI,CAACE,WAAU,GAAI,EAAE;MACvB,CAAC,CAAC;IACN,CAAC;IACDgB,UAAUA,CAACP,MAAM,EAAE;MACjBZ,KAAK,CAACoB,MAAM,CAAE,aAAYR,MAAO,EAAC,CAAC,CAACN,IAAI,CAAC,MAAM;QAC7C,IAAI,CAACJ,KAAI,GAAI,IAAI,CAACA,KAAK,CAACmB,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACR,EAAC,KAAMF,MAAM,CAAC;MAC5D,CAAC,CAAC;IACJ;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}